#include <stdio.h>

int max(int a, int b) { // func max that compares nums
    if (a > b) {
        return a;
    } else {
        return b;
    }
}
 int  main(){
    int a, b;
   // Enter nums
    printf("Enter nums to find max:\n");
    scanf("%d", &a);
    scanf("%d", &b);
   
    printf("Max is %d", max(a,b)); // Print result of func max
    return 0;
 }
